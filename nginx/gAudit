server {
      listen              443 ssl;
    server_name         gaudit.insomniac.technology;

    ssl_certificate /etc/nginx/ssl/nginx.crt;
    ssl_certificate_key /etc/nginx/ssl/nginx.key;

    location / {
        proxy_pass http://127.0.0.1:8000;
    }
    location /static {
        alias  /home/ubuntu/gAudit/app/static;
    }
}
